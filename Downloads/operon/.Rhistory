# Load required library
if (!requireNamespace("pheatmap", quietly = TRUE)) install.packages("pheatmap")
library(pheatmap)
# Generate sample data
set.seed(123)
data <- matrix(rnorm(100, mean = 5, sd = 2), nrow = 10, ncol = 10)
rownames(data) <- paste("Row", 1:10, sep = "")
colnames(data) <- paste("Col", 1:10, sep = "")
# Create the heatmap
pheatmap(data,
main = "Heatmap Example",
color = colorRampPalette(c("blue", "white", "red"))(50),
cluster_rows = TRUE,  # Cluster rows
cluster_cols = TRUE,  # Cluster columns
fontsize_row = 10,    # Font size for row labels
fontsize_col = 10,    # Font size for column labels
border_color = "black")

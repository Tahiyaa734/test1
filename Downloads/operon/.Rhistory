data <- data.frame(
Group = rep(c("A", "B", "C"), each = 10),
Values = c(rnorm(10, mean = 5, sd = 1),
rnorm(10, mean = 6, sd = 1.5),
rnorm(10, mean = 7, sd = 2))
)
# Create the boxplot
boxplot(Values ~ Group, data = data,
main = "Boxplot of Values by Group",
xlab = "Group",
ylab = "Values",
col = c("lightblue", "lightgreen", "lightpink"),
border = "darkblue")
# Add a grid
